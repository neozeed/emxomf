.TDEP	:= @O@libmoddef$A
.TLDF	:= -liberty
include mkexe.smak

.TARGET := emxaout.exe
.TKIND	:= $(TOOLFMT)
.TSRC	:= $(addprefix src/emxomf/,emxaout.c grow.c)
.TCF	:= $(CFLAGS.DEF.VERSION)
include mkexe.smak

.TARGET := listomf.exe
