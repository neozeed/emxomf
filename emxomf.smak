.MODULE	:= emxomf
.MDESC	:= a.out to OMF object file conversion utility
.INSDIR	:= bin/
.MDEP	:= omflib@omf moddef@omf

.TARGET	:= emxomf.exe
.TKIND	:= $(TOOLFMT)
.TSRC	:= $(addprefix src/emxomf/,emxomf.c stabshll.c grow.c)
.TCF	:= $(CFLAGS.DEF.VERSION)
.TDEP	:= @O@libomflib$A
.TLDF   := -liberty
include mkexe.smak

.TARGET	:= emxomfar.exe
.TKIND	:= $(TOOLFMT)
.TSRC	:= src/emxomf/emxomfar.c
